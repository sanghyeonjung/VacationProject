<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9F6E5"
    tools:context=".FirstStudyActivity">

    <ImageButton
        android:id="@+id/backButtonFirstStudyActivity"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/ic_undo"
        android:backgroundTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonRedFishStudyActivity"
        android:layout_width="196.51dp"
        android:layout_height="181.25dp"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="35dp"
        android:background="#D9F6E5"
        android:src="@drawable/ic_plant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonWhiteFishStudyActivity"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="255dp"
        android:background="#D9F6E5"
        android:src="@drawable/ic_whitefish"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonWhiteSlimeStudyActivity"
        android:layout_width="137dp"
        android:layout_height="82dp"
        android:layout_marginTop="404dp"
        android:layout_marginEnd="30dp"
        android:background="#D9F6E5"
        android:src="@drawable/ic_whiteslime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonHairStudyActivity"
        android:layout_width="213dp"
        android:layout_height="222dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="479dp"
        android:background="#D9F6E5"
        android:src="@drawable/ic_hair"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>